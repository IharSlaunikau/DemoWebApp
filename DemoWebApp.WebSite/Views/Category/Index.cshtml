@model IEnumerable<DemoWebApp.WebSite.Models.CategoryViewModel>
@{
    ViewData["Title"] = "Categories";
}

<div class="container">
    <h2 class="mb-4">Categories</h2>
    <div class="list-group">
        @foreach (var cat in Model)
        {
            <a href="#" class="list-group-item list-group-item-action">@cat.CategoryName</a>
        }
    </div>
</div>